{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cosmos-db-backup-test-df"
		},
		"CosmosDb1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDb1'"
		},
		"DestinationBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DestinationBlobStorage'"
		},
		"RestoreCosmosDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'RestoreCosmosDb'"
		},
		"RestoreDB2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'RestoreDB2'"
		},
		"SourceCosmosDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SourceCosmosDb'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "cosmos-db-backup-test-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CosmosDb1_connectionString": {
						"value": "[parameters('CosmosDb1_connectionString')]"
					},
					"DestinationBlobStorage_connectionString": {
						"value": "[parameters('DestinationBlobStorage_connectionString')]"
					},
					"RestoreCosmosDb_connectionString": {
						"value": "[parameters('RestoreCosmosDb_connectionString')]"
					},
					"RestoreDB2_connectionString": {
						"value": "[parameters('RestoreDB2_connectionString')]"
					},
					"SourceCosmosDb_connectionString": {
						"value": "[parameters('SourceCosmosDb_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}